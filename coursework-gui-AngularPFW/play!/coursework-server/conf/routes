# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample hello page
GET     /                           controllers.HomeController.index
#a get request to /testAnother calls the test method of TestController class of controllers package
GET     /testAnother                controllers.HomeController.testAnother

#Stuff used in the todo application
GET     /test                       controllers.TestController.test
#*name is a URL parameter
GET     /test/*name                 controllers.TestController.testWithParam(name : String)
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
